{
    "model": {
        "prefix": "config.model",
        "scope": "xml",
        "description": "Model configuration",
        "body": [
            "<model>",
            "\t<name>${1}</name>",
            "\t<version>1.0</version>",
            "\t<sdf version=\"1.6\">model.sdf</sdf>",
            "\t<author>",
            "\t\t<name>John Doe</name>",
            "\t\t<email>john.doe@email.com</email>",
            "\t</author>",
            "\t<description>",
            "\t\tModel description",
            "\t</description>",
            "</model>"
        ]
    },

    "name": {
        "prefix": "config.name",
        "scope": "xml",
        "description": "Model name",
        "body": [
            "<name>${1}</name>"
        ]
    },

    "version": {
        "prefix": "config.version",
        "scope": "xml",
        "description": "Model version",
        "body": [
            "<version>${1}</version>"
        ]
    },

    "author": {
        "prefix": "config.author",
        "scope": "xml",
        "description": "Author",
        "body": [
            "<author>",
            "\t<name>${1}</name>",
            "\t<email>${2}</email>",
            "</author>"
        ]
    },

    "description": {
        "prefix": "config.description",
        "scope": "xml",
        "description": "Model description",
        "body": [
            "<description>",
            "\t${1}",
            "</description>"
        ]
    },

    "sdf_version": {
        "prefix": "config.sdf_version",
        "scope": "xml",
        "description": "SDF version",
        "body": [
            "<sdf version=\"${1}\">model.sdf</sdf>"
        ]
    }
}