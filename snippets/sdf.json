{
	"pose": {
		"prefix": "sdf.pose",
		"scope": "xml",
		"description": "Pose (SDF 1.6)",
		"body": [
			"<pose>0 0 0 0 0 0</pose>"
		]
	},

	"pose_relative_to": {
		"prefix": "sdf.pose.relative_to",
		"scope": "xml",
		"description": "Pose relative to a frame (SDF 1.7)",
		"body": [
			"<pose relative_to=\"${1}\">0 0 0 0 0 0</pose>"
		]
	},

	"mass": {
		"prefix": "sdf.mass",
		"scope": "xml",
		"description": "Mass in kg",
		"body": [
			"<mass>${1}</mass>"
		]
	},

	"inertia": {
		"prefix": "sdf.inertia",
		"scope": "xml",
		"description": "Moments of inertia",
		"body": [
			"<inertia>",
			"\t<ixx>0</ixx>",
			"\t<ixy>0</ixy>",
			"\t<ixz>0</ixz>",
			"\t<iyy>0</iyy>",
			"\t<iyz>0</iyz>",
			"\t<izz>0</izz>",
			"</inertia>"
		]
	},

	"inertial": {
		"prefix": "sdf.inertial",
		"scope": "xml",
		"description": "Inertial block",
		"body": [
			"<inertial>",
			"\t<mass>0</mass>",
			"\t<inertia>",
			"\t\t<ixx>0</ixx>",
			"\t\t<ixy>0</ixy>",
			"\t\t<ixz>0</ixz>",
			"\t\t<iyy>0</iyy>",
			"\t\t<iyz>0</iyz>",
			"\t\t<izz>0</izz>",
			"\t</inertia>",
			"\t<pose>0 0 0 0 0 0</pose>",
			"</inertial>"
		]
	},

	"link_simple_1.6": {
		"prefix": "sdf.link.simple",
		"scope": "xml",
		"description": "Simple link (SDF 1.6)",
		"body": [
			"<link name=\"${1}\">",
			"\t<pose>0 0 0 0 0 0</pose>",
			"\t<collision>",
			"\t\t${2}",
			"\t</collision>",
			"",
			"\t<visual>",
			"\t\t${3}",
			"\t</visual>",
			"</link>"
		]
	},

	"link_simple_1.7": {
		"prefix": "sdf.link.simple.relative_to",
		"scope": "xml",
		"description": "Simple link (SDF 1.7)",
		"body": [
			"<link name=\"${1}\">",
			"\t<pose relative_to=\"${2}\">0 0 0 0 0 0</pose>",
			"\t<collision>",
			"\t\t${3}",
			"\t</collision>",
			"",
			"\t<visual>",
			"\t\t${4}",
			"\t</visual>",
			"</link>"
		]
	},

	"velocity_decay": {
		"prefix": "sdf.velocity_decay",
		"scope": "xml",
		"description": "[Link] Velocity decay",
		"body": [
			"<velocity_decay>",
			"\t<linear>0</linear>",
			"\t<angular>0</angular>",
			"</velocity_decay>"
		]
	},

	"include_1.6": {
		"prefix": "sdf.include",
		"scope": "xml",
		"description": "Include block (SDF 1.6)",
		"body": [
			"<include>",
			"\t<uri></uri>",
			"\t<name></name>",
			"\t<static>0</static>",
			"\t<pose>0 0 0 0 0 0</pose>",
			"</include>"
		]
	},

	"include_1.7": {
		"prefix": "sdf.include.relative_to",
		"scope": "xml",
		"description": "Include block (SDF 1.7)",
		"body": [
			"<include>",
			"\t<uri></uri>",
			"\t<name></name>",
			"\t<static>0</static>",
			"\t<pose relative_to=\"frame_name\">0 0 0 0 0 0</pose>",
			"</include>"
		]
	}

}